import java.util.Scanner;

public class Listaexercicioaula2203_exerc8 {
    

public class JavaApplicationEx8 {
   
    public static void main(String[] args) {
        System.out.println("Contagem regressiva de 10 a 1:");

        for (int i = 10; i >= 1; i--) {
            System.out.println(i);
        }
    }
}

}
